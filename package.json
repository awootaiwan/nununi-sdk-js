  "scripts": {
    "start": "parcel src/index.html --port 80  --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:demo": "parcel demo/index.html --port 1111 --open",
    "watch": "parcel watch src/index.html",
    "watch:app": "parcel watch src/app.js",
    "watch:co": "parcel watch src/components/relatedProduct/relatedProduct.js",
    "demo:style": "parcel build demo/css/header.scss --no-source-maps --out-dir demo/css",
    "build:app": "parcel build src/app.js --out-dir production --out-file nununi-sdk-latest.min.js --no-source-maps"
  },
  "homepage": "https://awoo.org",
